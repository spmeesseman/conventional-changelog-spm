{
  "name": "conventional-changelog-spm",
  "version": "1.2.3",
  "description": "conventional-changelog spm custom preset",
  "homepage": "https://github.com/spmeesseman/conventional-changelog-spm/tree/master/packages/conventional-changelog-spm#readme",
  "license": "MIT",
  "main": "index.js",
	"author": {
		"name": "Scott Meesseman"
	},
  "repository": {
    "type": "git",
    "url": "git+https://github.com/spmeesseman/conventional-changelog-spm.git"
  },
  "scripts": {
    "test-windows": "mocha --timeout 30000",
    "semantic-release": "semantic-release",
    "semantic-release-no-ci": "semantic-release --no-ci",
    "semantic-release-dry": "semantic-release --dry-run",
    "commit": "npx git-cz"
  },
  "bugs": {
    "url": "https://github.com/spmeesseman/conventional-changelog-spm/issues"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "compare-func": "^1.3.1",
    "q": "^1.5.1"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "semantic-release": "^16.0.0"
  },
  "engines": {
    "node": ">=6.9.0"
  },
  "files": [
    "conventional-changelog.js",
    "conventional-recommended-bump.js",
    "index.js",
    "parser-opts.js",
    "writer-opts.js",
    "templates"
  ],
  "keywords": [
    "conventional-changelog",
    "angular",
    "preset"
  ]
}
